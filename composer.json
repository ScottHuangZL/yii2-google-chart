{
    "name": "johnsnook/yii2-google-charts",
    "description": "A wrapper for google charts",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "google charts", "charts", "widget"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "John Snook",
            "email": "jsnook@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/johnsnook/yii2-google-chart/issues",
        "source": "https://github.com/johnsnook/yii2-google-chart"
    },
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "johnsnook\\googlechart\\": "/src"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/var/www/johnsnook/yii2-google-graph"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ] 
}
